<h2>ユーザー情報編集</h2>

<table>
  <%= form_with model: @user, url: user_path, method: :patch do |f| %>
  <tr>
    <td>ニックネーム</td>
    <td><%= f.text_field :name %></td>
  </tr>

  <tr>
    <td>メールアドレス</td>
    <td><%= f.text_field :email %></td>
  </tr>

  <tr>
    <td><%= f.submit "変更を保存" %></td>
  </tr>
  <% end %>
</table>